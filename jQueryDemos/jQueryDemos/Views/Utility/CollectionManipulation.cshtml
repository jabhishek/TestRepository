@{
    ViewBag.Title = "CollectionManipulation";
}

<h2>CollectionManipulation</h2>

<script>
    var myArray = [1, 3, 6, 1, 5, 7, 9];

    $(document).ready(function () {
        if ($.inArray(3, myArray) != -1) {
            $('div#out3').css('color', 'red');
            console.log('3 is in the array');
        }

        if ($.inArray(33, myArray) != -1) {
            $('div#out33').css('color', 'red');
            console.log('33 is in the array');
        }
        console.log(myArray);

        $.unique(myArray);
        console.log(myArray);

        $.merge(myArray, $.makeArray([10, 8, 9]));
        console.log(myArray);
        $.unique(myArray);
        console.log(myArray);

        var newArray = $.map(myArray, function (item, index)
        { return item * 2 });
        console.log(newArray);

        var greppedArray = $.grep(myArray, function (item) { return item % 3 == 0 });
        console.log(greppedArray);

        console.log($('div'));
        console.log($.makeArray($('div')));
        
    });
</script>

<div id="out1">1</div>
<div id="out2">2</div>
<div id="out3">3</div>
<div id="out4">4</div>
<div id="out5">5</div>
<div id="out6">6</div>
<div id="out7">7</div>
<div id="out8">8</div>
